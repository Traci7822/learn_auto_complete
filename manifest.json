{
  "manifest_version": 2,

  "name": "Learn Auto Complete",
  "description": "This extension allows you to save and retrieve commonly used content",
  "author":  "https://github.com/Traci7822",
  "version": "0.0.0.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Learn Auto-Complete"
  },

  "permissions": ["storage"],

  "content_scripts": [
    {
      "matches": ["https://learn.co/expert-chat"],
      "js": [ "index.js", "jquery-3.2.0.min.js" ],
      "css": ["index.css"]
    }
  ],

  "background": {
    "scripts": ["index.js"],
    "peristant": false
  },

  "web_accessible_resources": [
    "index.css",
    "icon.png"
  ],

  "icons":
    {
      "16": "icon.png",
      "48": "icon.png"
    }

}
